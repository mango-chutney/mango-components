{
  "env": {
    "es": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "usage",
            "modules": false,
            "loose": true
          }
        ],
        "@babel/preset-react",
        "@babel/preset-flow"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        ["styled-components", { "ssr": true, "displayName": false }]
      ]
    },
    "cjs": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "usage",
            "modules": "commonjs"
          }
        ],
        "@babel/preset-react",
        "@babel/preset-flow"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        ["styled-components", { "ssr": true, "displayName": false }]
      ]
    }
  }
}
